{
    "version": 2,
    "builds": [
      {
        "src": "src/app",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/**/*.ts": {
        "runtime": "@vercel/node"
      }
    }
  }
  